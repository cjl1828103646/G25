<script type="text/javascript">
    //建立XMLHttpRequest請求
    var  xmlhttp;
    if (Window.XMLHttpRequset){
          //使用與高級瀏覽器(IE7 FireFox，Chrome,Opera,Safari 瀏覽器瀏覽代碼)
        xmlhttp=new XMLHttpRequest();
    }else {
        //使用與ie5 ie6瀏覽器
        xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    }
    console.log(xmlhttp);
</script>
